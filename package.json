{
  "name": "boilerplate-react",
  "version": "1.0.0",
  "description": "boilerplate for react",
  "author": "pureainu@gmail.com",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config build/webpack.dev.conf.js",
    "clean": "rimraf dist",
    "prebuild:prod": "npm run clean",
    "build:prod": "cross-env NODE_ENV=production webpack -p --config build/webpack.prod.conf.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "bash .script/commit-msg.sh ${GIT_PARAMS}"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.8.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  }
}
